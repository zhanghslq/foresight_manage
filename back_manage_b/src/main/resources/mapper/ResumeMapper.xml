<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhs.backmanageb.mapper.ResumeMapper">

    <select id="countByCurrentStatusId" resultType="com.zhs.backmanageb.model.bo.CommonCountBO">
        select current_status_id as id,count(1) as count from resume
        where deleted=0
    </select>
</mapper>
